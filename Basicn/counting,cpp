  int count = 0;
        int original_n = n;  // Store the original number
        while (n > 0) {
            int digit = n % 10;  // Get the last digit
            n /= 10;  // Remove the last digit
            
            // Check if digit is not zero and if it divides the original number
            if (digit != 0 && original_n % digit == 0) {
                count++;
            }
        }
        return count;